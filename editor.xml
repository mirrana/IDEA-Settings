<application>
  <component name="CodeInsightSettings">
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <EXCLUDED_PACKAGE NAME="com.sasAll" />
    <EXCLUDED_PACKAGE NAME="net.compusult.wes.server.Collections.collections_xml_app" />
    <EXCLUDED_PACKAGE NAME="net.opengis" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="c06ca7" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="c06ca7" />
      <option name="ERROR_STRIPE_COLOR" value="c06ca7" />
    </pattern>
  </component>
</application>